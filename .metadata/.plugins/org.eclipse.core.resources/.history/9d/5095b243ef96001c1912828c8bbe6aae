package constructors;
	class Empno{
		int id;
		String name;

	void display() {
		System.out.println(id+" "+name);
		}
	}

	public class constructorDemo {

	public static void main(String[] args) {

		Empno e1=new Empno();
		Empno e2=new Empno();

		e1.display();
		e2.display();
		}
	}

	class employee{
		int id;
		String name;

		employee(int i,String n)
		{
		id=i;
		name=n;
		}

		void display() {
		System.out.println(id+" "+name);
		}
	public static void main(String[] args) {

		employee s1=new employee(1,"Ajay");
		employee s2=new employee(5,"Anjali");
		s1.display();
		s2.display();
			}
	}


